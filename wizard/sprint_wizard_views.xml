<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<record id="sprint_wizard_view_form" model="ir.ui.view">
		<field name="name">sprint.wizard.form</field>
		<field name="model">sprint.wizard</field>
		<field name="arch" type="xml">
			<form string="Create Sprint">
				<group>
					<field name="name"/>
					<field name="start_date"/>
					<field name="end_date"/>
				</group>
				<footer>
					<button name="create_sprint" string="Create Sprint" type="object" class="btn-primary"/>
					<button string="Cancel" class="btn-secondary" special="cancel"/>
				</footer>
			</form>
		</field>
	</record>
</odoo>